<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UserReg</servlet-name>
        <servlet-class>QRVoting.UserReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLog</servlet-name>
        <servlet-class>QRVoting.Logs.AdminLog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_election</servlet-name>
        <servlet-class>QRVoting.add_election</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_candidate</servlet-name>
        <servlet-class>QRVoting.add_candidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Getimage</servlet-name>
        <servlet-class>QRVoting.Elections.Getimage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserApprove</servlet-name>
        <servlet-class>QRVoting.Elections.UserApprove</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserReject</servlet-name>
        <servlet-class>QRVoting.Elections.UserReject</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_signin</servlet-name>
        <servlet-class>QRVoting.Logs.user_signin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserReg</servlet-name>
        <url-pattern>/UserReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLog</servlet-name>
        <url-pattern>/AdminLog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_election</servlet-name>
        <url-pattern>/add_election</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_candidate</servlet-name>
        <url-pattern>/add_candidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Getimage</servlet-name>
        <url-pattern>/Getimage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserApprove</servlet-name>
        <url-pattern>/UserApprove</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserReject</servlet-name>
        <url-pattern>/UserReject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_signin</servlet-name>
        <url-pattern>/user_signin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
